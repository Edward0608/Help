// Current image index
int currentImage = 0;

void setup() {
  // Initialize OLED display
  display.begin(SSD1306_SWITCHCAPVCC);
  display.clearDisplay();

  // Set button pins as input
  pinMode(upButton, INPUT);
  pinMode(downButton, INPUT);
}

void loop() {
  // Check button press
  if (digitalRead(upButton) == LOW) {
    currentImage++;
    if (currentImage >= 2) { // Assuming you have only 2 images
      currentImage = 0;
    }
    displayImage(currentImage);
  } else if (digitalRead(downButton) == LOW) {
    currentImage--;
    if (currentImage < 0) {
      currentImage = 1;
    }
    displayImage(currentImage);
  }
}

void displayImage(int imageIndex) {
  display.clearDisplay();
  switch (imageIndex) {
    case 0:
      display.drawBitmap(0, 0, epd_bitmap_Dashboard, 128, 64, WHITE);
      break;
    case 1:
      display.drawBitmap(0, 0, epd_bitmap_Start, 128, 64, WHITE);
      break;
         case 2:
      display.drawBitmap(0, 0, epd_bitmap_Stop, 128, 64, WHITE);
      break;
  }
  display.display();
}
